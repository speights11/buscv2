{
  "name": "buscv2",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20 || 21 || 22"
  },
  "jest": {
    "moduleNameMapper": {
      "swiper/css": "<rootDir>/node_modules/swiper/swiper.min.css",
      "swiper/css/navigation": "<rootDir>/node_modules/swiper/modules/autoplay/navigation.min.css",
      "swiper/css/pagination": "<rootDir>/node_modules/swiper/modules/autoplay/pagination.min.css"
    },
    "collectCoverageFrom": [
      "src/**/{!(registerServiceWorker),}.js"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "babel.config.js",
      "index.js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "babel-jest"
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(swiper|swiper/react|ssr-window|d3|internmap|dom7|react-countup|countup)/)"
    ],
    "snapshotSerializers": []
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.11.2",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vidstack/react": "^1.11.21",
    "aws-amplify": "^6.15.5",
    "babel-eslint": "^10.1.0",
    "babel-polyfill": "^6.26.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "luxon": "^3.7.1",
    "mui-image": "^1.0.9",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-lines-ellipsis": "^0.15.4",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "react-share": "^5.1.0",
    "react-spinners": "^0.13.8",
    "sass": "^1.90.0",
    "swiper": "^11.1.4",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "prod": "serve -s build",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "test-ci": "react-scripts --openssl-legacy-provider test --ci --env=jsdom --watchAll=false --updateSnapshot --silent",
    "eject": "react-scripts eject",
    "lint": "eslint --fix src/**/*.js",
    "install:clean": "rm -rf node_modules/ && rm -rf yarn.lock && yarn install && yarn audit",
    "query": "aws dynamodb scan --table-name ContactUs-7ht4ninxqnfdrne7hpnllko6by-main > email-table-contents.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "postcss": "^8.4.31",
    "nth-check": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "prettier": "3.1.0"
  }
}
